{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es6",
      "jsx": "react",
      "lib": ["dom", "es2017"],
      "allowJs": true,
      "noEmit": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["**/*.ts", "**/*.tsx"],
    "exclude": ["node_modules"]
  }
  